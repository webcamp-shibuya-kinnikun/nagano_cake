<%= link_to "新規登録", new_admins_item_path(), class: "btn btn-success"%>


<% @item.each do |i| %>
<%= i.genre.name %>
<%= link_to i.name, admins_item_path(i)  %>
<%= i.explanation %>
<%= i.tax_out_price %>
<%= attachment_image_tag i, :image, :fill, 40, 40, class: "pull-left profile-thumb" %>
<%= i.is_sale %>
<% end %>