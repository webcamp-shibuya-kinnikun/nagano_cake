<% if @delivary_address.errors.any? %>
  $('#error').html("<%= j(render 'shared/error_messages', resource: @delivary_address) %>");
<% else %>
  $('#create').html("<%= j(render 'public/delivary_addresses/delivary', delivary_addresses: @delivary_addresses) %>");
  $('#create').show();
  $('input').val("");
  $('#error_explanation').hide();
  $('#flash').html("<%= j(render 'layouts/flash') %>");
  $('#flash').show();
  setTimeout("$('#flash').fadeOut('slow')", 2000);
<% end %>